#include <iostream>
using namespace std;
#include <climits>

void findLargest(int **input, int nRows, int mCols)
{
     int largest = INT_MIN; 
     int index= 0;
     bool isRow=true; 
    for(int i = 0; i < nRows; i++){
        int sum = 0;
        for(int j = 0; j < mCols; j++){
            sum = sum + input[i][j];
        }
        if(sum > largest){
                largest = sum;
                index = i;
            }
    }
    for(int j = 0; j < mCols; j++){
       int add = 0;
        for(int i = 0; i < nRows; i++){
            add = add + input[i][j];
        }
        if(add > largest){
                largest= add;
                index = j;
                isRow=false;
            }
    }
    if(isRow){
        cout << "row" <<" "<<index <<" "<<largest;
    }
    else{
        cout << "column" <<" "<< index <<" "<< largest;
    }
}

int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		int row, col;
		cin >> row >> col;
		int **input = new int *[row];
		for (int i = 0; i < row; i++)
		{
			input[i] = new int[col];
			for (int j = 0; j < col; j++)
			{
				cin >> input[i][j];
			}
		}
		findLargest(input, row, col);
		cout << endl;
	}
}
